print("It being spammed")

local args = {
    [1] = Vector3.new(3991.5778808594, 28149.111328125, -956.24041748047)
}

game:GetService("Players").LocalPlayer.Character.NoMoreMutants.Fire:FireServer(unpack(args))




end)
 




