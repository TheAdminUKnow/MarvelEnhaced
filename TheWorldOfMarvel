local A_1 = "WandaHouseOfM"
 local Event = game:GetService("ReplicatedStorage").ChangePlayerCharacter
 Event:FireServer(A_1)
 local Event = game:GetService("Workspace").Teleport30.RemoteFunction
 Event:InvokeServer()

end)


