 local Miscc = Window:NewTab("Misc")
 local MisccSection = Miscc:NewSection("Misc Options")
 
 
 
MisccSection:NewButton("Destroy Buildings & Etc...", "destroy", function(currentOption)
       print(currentOption)
    
    game.Workspace.Map.MapBorder:destroy()
    game.Workspace.Map.Props:destroy()
    wait(0,5)
    game.Workspace.Map.Building:destroy()

end)

 MisccSection:NewDropdown("Sound Options", "No Music", {"SOUNDS OFF"}, function(s)
   
   game.ReplicatedStorage.GUI.City:destroy()
    game.ReplicatedStorage.GUI.Music:destroy()
    print(currentOption)
    
end)

    
    
