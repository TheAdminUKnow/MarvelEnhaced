local Window = Library.CreateLib("Heroes: Multiverse GUI by Nut The Menace", "DarkTheme")
 
 local Main = Window:NewTab("Main")
 local MainSection = Main:NewSection("Main Menu")
 
 
 MainSection:NewButton("Teleport to Main Area", "go to Main Area", function()
    print("Clicked")

--Main Area
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(31.6734467, 4.48320818, -36.4655228, 0.137778997, 8.60039648e-08, -0.990463018, 2.85523001e-08, 1, 9.08038658e-08, 0.990463018, -4.07908622e-08, 0.137778997)


