    game.Players.LocalPlayer.Character.Humanoid.BodyHeightScale:destroy()
    game.Players.LocalPlayer.Character.Humanoid.BodyWidthScale:destroy()
    game.Players.LocalPlayer.Character.Humanoid.HeadScale:destroy()
    
