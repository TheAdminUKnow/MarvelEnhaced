  MainSection:NewButton("*New* Invisible Mode with troll", "Invis Tp", function()
    print("Clicked")
    
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-990.234558, -124.59021, 641.530884, -0.0947015435, 7.6300303e-08, 0.995505691, 8.32636431e-08, 1, -6.87239776e-08, -0.995505691, 7.63811627e-08, -0.0947015435)
wait(1,2)
game.Players.LocalPlayer.Character.LowerTorso:destroy()
game.StarterGui:SetCore("SendNotification", { 
    Title = "Enerting Invis Mode";
