     -- PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
    PlayerSection:NewButton("Run fast as speed", "You stole Speed's power", function()
    print(currentOption)
    while true do 
    wait(0,5)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 121

end
  
    
   end)
   
     PlayerSection:NewSlider("Jumper Power", "Changes Your JumpPower", 500, 20, function(s)
       game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
   
  end)





  local Misc = Window:NewTab("Misc")
