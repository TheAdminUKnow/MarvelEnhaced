    if game.PlaceId == 6107938757 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Marvel Enhanced V.2 GUI + More by Nut The Menace", "DarkTheme")
   
     -- Character Change
    local InvisibleWoman = Window:NewTab("Invisible Woman")
    local InvisibleWomanSection = InvisibleWoman:NewSection("Invisible Woman's & Scarlet Witch's Power Spam")
     
    InvisibleWomanSection:NewButton("Force Bash", "Power 1", function(txt)
    print("txt")
   local A_1 = "Invisible Woman"
   local Event = game:GetService("ReplicatedStorage").Remotes.Sound
   Event:InvokeServer(A_1) 
   local A_1 = "ShieldBash"
   local A_2 = CFrame.new(-10311.8096, 558.986633, -1250.23999, 0.99954164, -0.00034183127, -0.030272413, -2.91038305e-11, 0.999936223, -0.0112911221, 0.0302743427, 0.0112859467, 0.999477863)
   local Event = game:GetService("ReplicatedStorage").Remotes.InvisibleWomanAbilities
   Event:InvokeServer(A_1, A_2)
   
  end)






    InvisibleWomanSection:NewButton("Go Invisible", "Power 2", function(txt)
    print("txt")
    
    

local args = {
    [1] = "InvisibleStart",
    [2] = game:GetService("Players").LocalPlayer.Character
}

game:GetService("ReplicatedStorage").Remotes.InvisibleWomanAbilities:InvokeServer(unpack(args))

  end)

  


    InvisibleWomanSection:NewDropdown("Become Visible", "Invisibility OFF",{"Turns Invisiblity Off"}, function(currentOption)
    

local args = {
    [1] = "InvisibleEnd",
    [2] = game:GetService("Players").LocalPlayer.Character
}

game:GetService("ReplicatedStorage").Remotes.InvisibleWomanAbilities:InvokeServer(unpack(args))

end)



   InvisibleWomanSection:NewButton("Shield Somone - Working on it now", "Type person name to shield them", function(txt)
	print(txt)

   local A_1 = "Shield"
   local A_2 = game:GetService("Workspace") [""]
   local Event = game:GetService("ReplicatedStorage").Remotes.InvisibleWomanAbilities
   Event:InvokeServer(A_1, A_2)
    
end)



          
    InvisibleWomanSection:NewButton("Scarlet Witch's Shield (Shields you)", "Shield yourself", function(txt)
	print(txt)

   local A_1 = "ShieldStart"
   local A_2 = CFrame.new(-10119.2539, 552.394409, -384.920624, 0.00271626026, -7.96572195e-08, 0.999996305, 2.04343031e-09, 1, 7.96519615e-08, -0.999996305, 1.82706728e-09, 0.00271626026)
