	print(txt)

    while true do wait(0,5)

    local A_1 = "HexThrow"
    local A_2 = CFrame.new(-10203.3926, 553.416992, -431.358765, -0.758618534, -0.183600843, 0.625131011, 1.49011612e-08, 0.959474087, 0.28179732, -0.651535153, 0.213776678, -0.727874696)
